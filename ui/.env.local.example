# Environment Variables Template
# Copy this file to .env.local and update with your actual values

# Builder.io Configuration
# Get your API key from https://builder.io/account/settings
VITE_BUILDER_API_KEY=

# Builder.io URLs (optional, auto-detected if not set)
VITE_BUILDER_PREVIEW_URL=http://localhost:5173
VITE_BUILDER_EDIT_URL=http://localhost:5173?builder.edit=true

# Vite Development Server
VITE_PORT=5173
VITE_HOST=0.0.0.0

# Optional: Backend API (if different from localhost)
VITE_API_URL=http://localhost:8888
